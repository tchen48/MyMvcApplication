@model MyMvcApplication.Models.Employee

@{
    ViewBag.Title = "Employee Details";
}

<h2>Employee Details</h2>
<div style="font-family:Arial">
    <table>
        <tr>
            <td>
                <b>Emaployee Id :</b>
            </td>
            <td>
                @Model.EmployeeId
            </td>
        </tr>
        <tr>
            <td>
                <b>Name :</b>
            </td>
            <td>
                @Model.Name
            </td>
        </tr>
        <tr>
            <td>
                <b>Gender :</b>
            </td>
            <td>
                @Model.Gender

            </td>
        </tr>
        <tr>
            <td>
                <b>City :</b>
            </td>
            <td>
                @Model.City
            </td>
        </tr>
    </table>
    @Html.ActionLink("Back to Index", "Index", new {departmentId=@Model.DepartmentId })
</div>